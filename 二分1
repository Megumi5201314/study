#include<stdio.h>
#include<math.h>

int main() {
	int a[1007] = { 0 }, map[2007] = {0};
	int n, x, c;
	scanf("%d%d", &n, &x);
	for (int i = 0; i < n; i++) {
		scanf("%d", a+i);
		map[a[i] + 1000] = 1;
	}

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			c = -(a[i] * x * x + a[j] * x);
			if (abs(c) <= 1000 && map[c + 1000]) {
				printf("YES");
				return 0;
			}
		}
	}
	printf("NO");
	return 0;

}
